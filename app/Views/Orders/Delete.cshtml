@model app.Models.Order
@{
    ViewBag.Title = "DeleteOrder";
}
<table>
    <tr>       
        <td>User</td>
        <td>Address</td>
    </tr>
    <tr>
        <td>@Html.DisplayFor(model => model.User)</td>
        <td>@Html.DisplayFor(model => model.Address)</td>
    </tr>
</table>
<div>
    <div>
        <form asp-controller="Orders" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Delete" />
            </div>
        </form>
    </div>
</div>