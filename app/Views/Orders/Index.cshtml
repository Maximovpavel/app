@model IEnumerable<app.Models.Order>
    @{
        ViewBag.Title = "Orders";
}
<h3>Orders</h3>
<table>
    <tr>
       <td>User</td>
       <td>Address</td>
       <td>Contact Phone</td>
       <td></td>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.User</td>
            <td>@order.Address</td>
            <td>@order.ContactPhone</td>
            <td><a asp-action="Delete" asp-controller="Orders" asp-route-id="@order.OrderId">Delete</a></td>
        </tr>
    }
</table>
